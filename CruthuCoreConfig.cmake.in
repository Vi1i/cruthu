# - Config file for the FooBar package
# It defines the following variables
#  CRUTHU_INCLUDE_DIRS - include directories for Cruthu
#  CRUTHU_LIBRARIES    - libraries to link against
#  CRUTHU_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(CRUTHU_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(CRUTHU_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET Cruthu CruthuCoreTeraGenerate AND NOT Cruthu_BINARY_DIR)
    include("${CRUTHU_CMAKE_DIR}/CruthuCoreTargets.cmake")
endif()

# These are IMPORTED targets created by CruthuTargets.cmake
set(CRUTHU_LIBRARIES Cruthu CruthuCoreTeraGenrate)
#set(FOOBAR_EXECUTABLE bar)
